__sub__ numbers(integer n){
    while(n GT 0){
        print n
        n = n SUB 1
    }
}


__function__ integer Fatorial(integer n){
    if (n = 0){
        Fatorial = 1
    }

    else{
        Fatorial = n MUL Fatorial(n SUB 1)
    }
}
    

__sub__ main(){
    __var__ integer n
    __var__ boolean flag
    

    n    = input
    flag = True

    if (flag){
        CALL numbers(n) 'componentes do fatorial
    }

    print Fatorial(n) 'resultado do fatorial
}